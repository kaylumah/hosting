{
  "Metadata": {
    "FallbackOutputLocation": "/:year/:month/:day/:name:ext",
    "OutputLocationMapping": {
      "posts2": "/blog/:year/:month/:day/:name:ext"
    },
    "ExtensionMapping": {
      ".md": ".html"
    },
    "Defaults": [
      {
        "Path": "",
        "Extensions": [
          ".txt",
          ".js",
          ".css",
          ".json"
        ],
        "Values": {
          "type": "Static"
        }
      },
      {
        "Path": "",
        "Extensions": [
          ".html"
        ],
        "Values": {
          "author": "max",
          "organization": "kaylumah",
          "image": "/assets/images/social_preview.png",
          "sitemap": true,
          "feed": false,
          "featured": false,
          "language": "en",
          "ads": false,
          "type": "Page"
        }
      },
      {
        "Scope": "posts",
        "Path": "",
        "Extensions": [
          ".html"
        ],
        "Values": {
          "layout": "post.html",
          "type": "Article",
          "socialshare": true,
          "feed": true,
          "ads": true
        }
      },
      {
        "Scope": "talks",
        "Path": "",
        "Extensions": [
          ".html"
        ],
        "Values": {
          "layout": "post.html",
          "type": "Talk",
          "socialshare": true,
          "feed": true,
          "ads": true,
          "custom": false
        }
      }
    ]
  },
  "Site": {
    "Lang": "en",
    "Baseurl": null,
    "Url": "https://kaylumah.nl",
    "Title": "Max Hamulyák · Kaylumah",
    "Description": "A blog and portfolio website for Kaylumah, a company founded by Max Hamulyák",
    "Collections": [
      {
        "Name": "posts",
        "Output": true
      },
      {
        "Name": "talks",
        "Output": true
      },
      {
        "Name": "drafts",
        "Output": false,
        "TreatAs": "posts"
      }
    ],
    "SupportedFileExtensions": [
      ".md",
      ".html",
      ".xml",
      ".css",
      ".js",
      ".json",
      ".txt"
    ],
    "SupportedDataFileExtensions": [
      ".yml",
      ".json",
      ".csv"
    ]
  }
}