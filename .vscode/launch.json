{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console) Generator",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-sitegenerator",
            "program": "${workspaceFolder}/artifacts/bin/Kaylumah.Ssg.Client.SiteGenerator/Debug/net6.0/Kaylumah.Ssg.Client.SiteGenerator.dll",
            "args": [
                "Site:Url=http://localhost:8080"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch (console) Validator",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-sitevalidator",
            "program": "${workspaceFolder}/artifacts/bin/Kaylumah.Ssg.Client.SiteValidator/Debug/net6.0/Kaylumah.Ssg.Client.SiteValidator.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}