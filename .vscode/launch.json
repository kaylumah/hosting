    "version": "0.2.0",
    "configurations": [
        {
            "name": "SiteGenerator.Development",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-sitegenerator",
            "program": "${workspaceFolder}/src/Component/Client/SiteGenerator/bin/Debug/net7.0/Kaylumah.Ssg.Client.SiteGenerator.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "launchSettingsProfile": "Kaylumah.Ssg.Client.SiteGenerator.Development",
            "launchSettingsFilePath": "${workspaceFolder}/src/Component/Client/SiteGenerator/Properties/launchSettings.json",
             "logging": {
                 "moduleLoad": false
             }
        },
        {
            "name": ".NET Core Launch (console) Validator",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-sitevalidator",
            "program": "${workspaceFolder}/src/Component/Client/SiteValidator/bin/Debug/net7.0/Kaylumah.Ssg.Client.SiteValidator.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}